  "main": "dist/server.js",
  "scripts": {
    "start-old": "concurrently 'nodemon -w server/server.js server/server.js' 'npm run watch'",
    "migrate": "mongo server/scripts/init.mongo.js",
    "webpack-old": "npm run compile && node_modules/.bin/webpack static/App.js -o static/app.bundle.js",
    "build": "node_modules/.bin/webpack",
    "watch": "node_modules/.bin/webpack-dev-server --hot --inline",
    "start": "node_modules/.bin/webpack-dev-server",
    "compile-server": "node_modules/.bin/babel server --presets=@babel/preset-env --out-dir dist --source-maps",
    "watch-server": "node_modules/.bin/babel server --presets=@babel/preset-env --out-dir dist --source-maps --watch",
    "start-hook": "nodemon -w server server/start_hook.js",
    "start-server": "nodemon -w dist dist/server.js",
    "dev-all": "concurrently 'npm run watch' 'npm run watch-server' 'npm start-server'",
    "dev-all-hook": "concurrently 'npm run watch' 'npm run start-hook'"
  },
